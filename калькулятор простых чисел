<iostream> // директива препроцессора
/* пример программы, подсчитывающей сумму двух целых чисел, значения которых вводятся с клавиатуры, а результат выводится на экран */ 
using namespace std;
int main()
{
  int a,b;
  count <<"Введите 2 целых числа" << endl;
  cin >> a >> b;
  cout <<"Их  сумма равна " << a + b;
}
return 0
